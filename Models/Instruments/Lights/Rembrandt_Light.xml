  </animation>
 
 
  <animation>
    <type>light</type>
    <light-type>spot</light-type>
    <name>AbblendL</name>
    <object-name>volume-AL</object-name>
    <position>
      <x>0.093</x>
      <y>-0.534</y>
      <z>0.363</z>
    </position>
    <direction>
      <x> -17.60 </x>
      <y>  -1.52 </y>
      <z>-0.556 </z>
    </direction>
    <ambient>
      <r>0.03</r>
      <g>0.03</g>
      <b>0.03</b>
      <a>1.0</a>
    </ambient>
    <diffuse>
      <r>1</r>
      <g>1</g>
      <b>1</b>
      <a>1.0</a>
    </diffuse>
    <specular>
      <r>1</r>
      <g>1</g>
      <b>1</b>
      <a>1.0</a>
    </specular>
    <attenuation>
      <c>1.0</c>
      <l>0.002</l>
      <q>0.00005</q>
    </attenuation>            
    <exponent>100</exponent> <!--Thus, higher spot exponents result in a more focused light source 0-128-->
    <cutoff>40</cutoff>
    <!--the maximum spread angle of a light source 0-90-->
    <near-m>0.05</near-m>
    <far-m>20</far-m>
  </animation>
  
