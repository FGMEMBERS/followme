<?xml version="1.0"?>

<PropertyList>
  
  <path>red_lights.ac</path>
  
  <animation>
    <type>select</type>    
    <object-name>red_lights</object-name>
    <condition>
      <equals>
	<property>sim/multiplay/generic/int[19]</property>
	<value>1</value>
      </equals>
    </condition> 
  </animation>
  
  <effect>
    <inherits-from>Effects/model-transparent</inherits-from>
    <object-name>red_lights</object-name>
    <condition>
      <property>sim/rendering/rembrandt/enabled</property>
    </condition>
  </effect>      
  
  <animation>
    <type>material</type>    
    <object-name>red_lights</object-name>
    <emission>
      <red>1</red>
      <green>0</green>
      <blue>0</blue>
      <factor-prop>sim/multiplay/generic/int[19]</factor-prop>
    </emission>
  </animation>  
  
  </PropertyList>
